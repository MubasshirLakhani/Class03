void main() {
 int num1=60;
if(num1 % 12 == 0 || num1 % 15 == 0){
  if(num1 % 12 == 0 && num1 % 15 == 0){
    print("GIVEN = $num1 ,THE GIVEN NUMBER IS DIVISIBLE BY BOTH 12 AND 15");
  }
  else if (num1 % 12 == 0){
    print("GIVEN = $num1 ,THE GIVEN NUMBER IS NOT DIVISIBLE BY 12");
  }
  else if(num1 % 15 == 0){
    print("GIVEN = $num1 ,THE GIVEN NUMBER IS NOT DIVISIBLE BY 15");
  }
}
  else {
    print('GIVEN = $num1 ,THE GIVEN NUMBER IS NOT DIVISIBLE BY 12 AND 15');
  }
 
}
